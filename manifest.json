{
  "manifest_version": 3,
  "name": "Nitro SEO Schema",
  "version": "2.0.1",
  "description": "Generate structured data schemas for SEO - Article, Product, Event, Recipe, Video, FAQ, LocalBusiness, Organization, and more",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Open Nitro SEO Schema"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}